#!/bin/bash

exec sed -e 's/α/\\alpha /g;
	s/β/\\beta /g;
	s/ζ/\\zeta /g;
	s/ε/\\epsilon /g;
	s/\\Π/\\prod /g;
	s/π/\\pi /g;
	s/Π/\\Pi /g;
	s/ψ/\\psi /g;
	s/Ψ/\\Psi /g;
	s/ξ/\\xi /g;
	s/Ξ/\\Xi /g;
	s/φ/\\varphi /g;
	s/Φ/\\Phi /g;
	s/ω/\\omega /g;
	s/Ω/\\Omega /g;
	s/δ/\\delta /g;
	s/Δ/\\Delta /g;
	s/ρ/\\rho /g;
	s/κ/\\kappa /g;
	s/Κ/\\Kappa /g;
	s/\\Σ/\\sum /g;
	s/σ/\\sigma /g;
	s/Σ/\\Sigma /g;
	s/Π/\\Pi /g;
	s/γ/\\gamma /g;
	s/Γ/\\Gamma /g;
	s/χ/\\chi /g;
	s/λ/\\lambda /g;
	s/Λ/\\Lambda /g;
	s/τ/\\tau /g;
	s/μ/\\mu /g;
	s/ν/\\nu /g;
	s/Ν/\\Nu /g;
	s/ι/\\iota /g;
	s/Ι/\\Iota /g;
	s/°/^\\circ /g;
	s/‒/{--}/g;
	s/„/{\\quotedblbase}/g;
	s/“/{\\textquotedblleft}/g;

	s/@</\\left< /g;
	s/@>/\\right> /g;
	s/@(/\\left( /g;
	s/@)/\\right) /g;
	s/@{/\\left\\{ /g;
	s/@}/\\right\\} /g;
	s/@\[/\\left[ /g;
	s/@\]/\\right] /g;

	s/≤/\\leq /g;
	s/≥/\\geq /g;
	s/∀/\\forall /g;
	s/∃/\\exists /g;
	s/∈/\\in /g;
	s/@⊂/\\subseteq /g;
	s/⊂/\\subset /g;
	s/∉/{\\not \\in} /g;
	s/@⊄/{\\not \\subseteq} /g;
	s/⊄/{\\not \\subset} /g;
	s/☢/{\\Radioactivity} /g;
	s/☣/{\\Biohazard} /g;
	s/☠/\skull /g;
	s/…/\\ldots /g;
	s/···/\\cdots /g;
	s/·/\\cdot /g;
	s/→•/\\begin{itemize} /g;
	s/←•/\\end{itemize} /g;
	s/↑•/\\begin{enumerate} /g;
	s/↓•/\\end{enumerate} /g;
	s/•/\\item  /g;
	s/◦/\\item[$\\circ$]  /g;
	s/→→/\\Rightarrow /g;
	s/←←/\\Leftarrow /g;
	s/←-→/\\leftrightarrow /g;
	s/←→/\\Leftrightarrow /g;
	s/-→/\\longrightarrow /g;
	s/→/\\to /g;
	s/←/\\leftarrow /g;
	'
