#!/bin/bash

exec sed -e 's/α/{\\ensuremath{\\alpha}}/g;
	s/β/{\\ensuremath{\\beta}}/g;
	s/ε/{\\ensuremath{\\epsilon}}/g;
	s/\\Π/\\prod/g;
	s/π/{\\ensuremath{\\pi}}/g;
	s/Π/{\\ensuremath{\\Pi}}/g;
	s/ψ/{\\ensuremath{\\psi}}/g;
	s/Ψ/{\\ensuremath{\\Psi}}/g;
	s/ξ/{\\ensuremath{\\xi}}/g;
	s/Ξ/{\\ensuremath{\\Xi}}/g;
	s/φ/{\\ensuremath{\\varphi}}/g;
	s/Φ/{\\ensuremath{\\Phi}}/g;
	s/ω/{\\ensuremath{\\omega}}/g;
	s/Ω/{\\ensuremath{\\Omega}}/g;
	s/δ/{\\ensuremath{\\delta}}/g;
	s/Δ/{\\ensuremath{\\Delta}}/g;
	s/ρ/{\\ensuremath{\\rho}}/g;
	s/κ/{\\ensuremath{\\kappa}}/g;
	s/Κ/{\\ensuremath{\\Kappa}}/g;
	s/\\Σ/\\sum/g;
	s/σ/{\\ensuremath{\\sigma}}/g;
	s/Σ/{\\ensuremath{\\Sigma}}/g;
	s/Π/{\\prod}/g;
	s/γ/{\\ensuremath{\\gamma}}/g;
	s/Γ/{\\ensuremath{\\Gamma}}/g;
	s/χ/{\\ensuremath{\\chi}}/g;
	s/λ/{\\ensuremath{\\lambda}}/g;
	s/Λ/{\\ensuremath{\\Lambda}}/g;
	s/τ/{\\ensuremath{\\tau}}/g;
	s/μ/{\\ensuremath{\\mu}}/g;
	s/ι/{\\ensuremath{\\iota}}/g;
	s/Ι/{\\ensuremath{\\Iota}}/g;
	s/°/{\\ensuremath{^\\circ}}/g;

	s/@</\\left</g;
	s/@>/\\right>/g;
	s/@(/\\left(/g;
	s/@)/\\right)/g;
	s/@{/\\left\\{/g;
	s/@}/\\right\\}/g;
	s/@\[/\\left[/g;
	s/@\]/\\right]/g;

	s/≤/{\\ensuremath{\\leq}}/g;
	s/≥/{\\ensuremath{\\geq}}/g;
	s/∀/{\\ensuremath{\\forall}}/g;
	s/∃/{\\ensuremath{\\exists}}/g;
	s/∈/{\\ensuremath{\\in}}/g;
	s/@⊂/{\\ensuremath{\\subseteq}}/g;
	s/⊂/{\\ensuremath{\\subset}}/g;
	s/∉/{\\ensuremath{\\not \\in}}/g;
	s/@⊄/{\\ensuremath{\\not \\subseteq}}/g;
	s/⊄/{\\ensuremath{\\not \\subset}}/g;
	s/☢/{\\Radioactivity}/g;
	s/☣/{\\Biohazard}/g;
	s/☠/{\\ensuremath{\skull}}/g;
	s/…/{\\dots}/g;
	s/···/{\\cdots}/g;
	s/·/{\\cdot}/g;
	s/→•/\\begin{itemize}/g;
	s/←•/\\end{itemize}/g;
	s/•/\\item /g;
	s/◦/\\item[$\\circ$] /g;
	s/→→/{\\ensuremath{\\Rightarrow}}/g;
	s/←→/{\\ensuremath{\\Leftrightarrow}}/g;
	s/-→/{\\ensuremath{\\longrightarrow}}/g
	s/→/{\\ensuremath{\\to}}/g
	'
